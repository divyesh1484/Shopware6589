<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="IctBlog1\Core\Content\IctBlog1\IctBlogDefinition">
            <tag name="shopware.entity.definition" entity="ict_blog"/>
        </service>

        <service id="IctBlog1\Core\Content\IctBlog1\ProductBlogMappingDefinition">
            <tag name="shopware.entity.definition" entity="product_blog"/>
        </service>

        <service id="IctBlog1\Core\Content\IctCategory\IctCategoryDefinition">
            <tag name="shopware.entity.definition" entity="blog_category"/>
        </service>

        <service id="IctBlog1\Core\Content\IctBlog1\Aggregate\IctBlogTranslation\IctBlogTranslationDefinition">
            <tag name="shopware.entity.definition" entity="ict_blog_translation"/>
        </service>

        <service id="IctBlog1\Core\Content\IctCategory\Aggregate\IctCategoryTranslation\IctCategoryTranslationDefinition">
            <tag name="shopware.entity.definition" entity="blog_category_translation"/>
        </service>
        
        <service id="IctBlog1\Core\Content\Extension\LanguageExtension">
            <tag name="shopware.entity.extension"/>
        </service>

        <service id="IctBlog1\Core\Content\Extension\CategoryExtension">
            <tag name="shopware.entity.extension"/>
        </service>

        <service id="IctBlog1\Core\Content\Extension\ProductExtension">
            <tag name="shopware.entity.extension"/>
        </service>

    </services>
</container>