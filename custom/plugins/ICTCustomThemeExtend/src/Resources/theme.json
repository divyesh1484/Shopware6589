{
  "name": "ICTCustomThemeExtend",
  "author": "ICT Custom Theme Extend",
  "views": [
     "@Storefront",
     "@Plugins",
     "@ICTCustomTheme",
     "@ICTCustomThemeExtend"
  ],
  "style": [
    "app/storefront/src/scss/overrides.scss",
    "@Storefront",
    "@ICTCustomTheme",
    "app/storefront/src/scss/base.scss"
  ],
  "script": [
    "@Storefront",
    "@ICTCustomTheme",
    "app/storefront/dist/storefront/js/i-c-t-custom-theme-extend.js"
  ],
  "asset": [
    "@Storefront",
    "@ICTCustomTheme",
    "app/storefront/src/assets"
  ],
  "configInheritance": [
    "@Storefront",
    "@ICTCustomTheme"
  ],
  "config": {
    "fields": {
      "sw-advent-calendar-background-color": {
        "label": {
          "en-GB": "Advent calendar background color",
          "de-DE": "Adventskalender Hintergrundfarbe"
        },
        "type": "color",
        "value": "#399",
        "editable": true
      }
    }
  }
}